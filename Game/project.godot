; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="ld-51"
run/main_scene="res://game/Menu.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://game/Global.tscn"

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[layer_names]

2d_physics/layer_1="Everything"
2d_physics/layer_2="Player"
2d_physics/layer_3="Arrow"
2d_physics/layer_4="Apple"
2d_physics/layer_5="Wall"

[mono]

project/assembly_name="ld-51"

[physics]

common/physics_fps=120
common/enable_pause_aware_picking=true
2d/physics_engine="GodotPhysics"
2d/default_gravity=980
2d/default_linear_damp=0.0
2d/default_angular_damp=0.0

[rendering]

environment/default_environment="res://default_env.tres"
